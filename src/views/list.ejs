<!DOCTYPE html>
<html lang="pt-br">
    <% include('partials/head.ejs') %> 
<body>
    <h1><%= title %></h1>

    <%- include('partials/navbar.ejs') %> 

    <ul>
        <% users.forEach((user) => { %>
            <li>
                <%= user.name  %> - idade: <%= user.age  %> - <%= user.email  %> - 
                <a href="/edit?id=<%= user._id%>">[editar]</a>             
            </li>
        <% }) %>   
    </ul>    
    <%- include('partials/foot.ejs') %> 

</body>
</html> 